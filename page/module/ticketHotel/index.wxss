page{
  background: #eee;
}
/*tab*/
.tabLine{
  width: 100%;
  height: 80rpx;
  display: flex;
  background: #9980DB;
  color: #fff;
}
.tabItem{
  width: 100%;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  margin: 0 15rpx;
}
.active{
  background: #fff;
  color: #9980DB;
  font-weight: bold;
}
/*member*/
.memberWrap{
  width: 100%;
  height: 160rpx;
  display: flex;
  background: #fff;
}
.member{
  width: 100%;
  height: 120rpx;
  display: flex;
  background: #fff;
  margin: 20rpx 0;
}
.member_item{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}
.member_item:nth-of-type(2){
  border-left: 1px solid #eee;
  border-right: 1px solid #eee;
}
 .numChange{
    width: 150rpx;
    height: 50rpx;
    border-radius: 5rpx;
    border: 1px solid #e4e4e4;
    display: flex;
    align-items: center;
    justify-content: center;
  }
  .numChange image{
    width: 30rpx;
    height: 30rpx;
    margin: auto;
  }
  .num{
    width: 50rpx;
    height: 50rpx;
    text-align: center;
    line-height: 50rpx;
    border-left: 1px solid #e4e4e4;
    border-right: 1px solid #e4e4e4;
  }
  /*main*/
  .main{
    width: 100%;
    height: auto;
    background: #eee;
    padding-top: 20rpx;
  }
  .mainBlock{
    width: 700rpx;
    height: auto;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    background: #fff;
    padding: 10rpx 10rpx;
    margin: 0 auto;
    position: relative;
    overflow: hidden;
    border-radius: 10rpx;

  }
  .block_item{
    width: 100%;
    height: 120rpx;
    display: flex;
  }
  .sideBan{
    width: 150rpx;
    height: 150rpx;
    background: #6bc2fa;
    position: absolute;
    top: -75rpx;
    left: -75rpx;
    transform:rotate(-45deg);
    text-align: center;
    line-height: 250rpx;
    color: #fff;
  }

  /*ticketTab*/
  .ticketTab{
    width: 100%;
    height: 120rpx;
    display: flex;
  }
  .ticketTab text{
    width: 100%;
    line-height: 120rpx;
    text-align: center;
  }
  .Tactive{
    border-bottom: 2px solid pink;
  }
  .choose text{
    width: 100%;
    height: 120rpx;
    line-height: 120rpx;
    text-align: center
  }