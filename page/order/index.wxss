page{
  background: #99a5ae;
}
.travelLine{
  width: calc(100% - 20rpx);
  height: auto;
  /*height: 80rpx;*/
  background: #fff;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  /*line-height: 40rpx;*/
  padding: 10rpx;
}
.travelLine text{
  font-weight: bold;
}
.dateInfo{
  width: calc(100% - 20rpx);
  height: 50rpx;
  padding: 10rpx;
  background: #fff;
  /*background: #f6f6f6;*/
  line-height: 50rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  border-top: 20rpx solid #f6f6f6;
}
.dateInfo text{
  margin-right: 15rpx;
}
.memberSum{
  /*border-top: 20rpx solid #f6f6f6;*/
  }
.information{
  width: 720rpx;
  height: auto;
  margin: 20rpx auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.mgT{
  margin-bottom: 100rpx;
}
.travelMember{
  width: 700rpx;
  height: auto;
  background: #fff;
  margin: 20rpx auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding: 5rpx 10rpx;
  border-radius: 10rpx;
}
.info_Item{
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.itemColumTit{
  width: 100rpx;
  height: 50rpx;
}
.columTit{
  width: 100%;
  font-weight: bold;
}
.insurance{
  width: 650rpx;
}
.info_Item image{
  width: 50rpx;
  height: 50rpx;
}
.info_Item  input{
  width: 550rpx;
  height: 50rpx;
}
.waitInput{
  width: 550rpx;
  height: 50rpx;
}
  /*footer_total*/
.footer_total{
    width:100%;
    height:80rpx;
    display:flex;
    flex-direction:row;
    align-items: center;
    justify-content:space-between;
    position:fixed;
    bottom:0;
    background: #fff;
    border-top: 1rpx solid #eee;
    z-index: 999999;
}
.toatl_price{
    width: 510rpx;
    height: 100%;
    display:flex;
    flex-direction:row;
    align-items: center;
    justify-content:flex-end;
    color: #f60;
    padding-right: 20rpx;
}
.submitBt{
    width: 220rpx;
    height: 100%;
    background: #f60;
    font-size: 36rpx;
    color: #fff;
    display:flex;
    flex-direction:row;
    align-items: center;
    justify-content:center;
    border-radius: 0;
}