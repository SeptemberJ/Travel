/*searchBlock*/
.searchBlock{
  width: 100%;
  height: 120rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #9980DB;
}
.searchBox{
  width: 710rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  background: #fff;
  overflow: hidden;
  border-radius: 5rpx;
}
.searchInput{
  width: 530rpx;
  /*width: 450rpx;*/
  height: 80rpx;
}
.searchInput input{
  width: 100%;
  height: 80rpx;
}
.searchIcon{
  width: 80rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.searchIcon image{
  width: 50rpx;
  height: 50rpx;
}
.searchClear{
  width: 80rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.searchClear image{
  width: 50rpx;
  height: 50rpx;
}
.searchGo{
  width: 100rpx;
  height: 80rpx;
  background: #f5f5f5;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #1296db;
}
/*searchResult*/
.searchResult{
  width: 710rpx;
  height: auto;
  padding: 20rpx;
}
.result_item{
  width: 710rpx;
  height: 100rpx;
  border-bottom: 1px solid #eee;
  /*overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;*/
}
.result_item navigator{
  width: 710rpx;
  height: 100rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.result_item text{
  width: 710rpx;
  height: 100rpx;
  line-height: 100rpx;
  
  /*padding: 0 10rpx;*/
}
/*sort*/
.sort{
  width: 100%;
  height: 80rpx;
  background: #ddd;
  display: flex;
  align-items: center;
}
.sort_kind{
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.sort_kind:not(:last-of-type){
  border-right: 1px solid #eee;
}
/*destinationList*/
.destinationList{
  width: 750rpx;
  height: calc(100% - 120rpx);
  display: flex;
  flex-direction: row;
  align-items:flex-start;
}
.menu_block{
  width: 150rpx;
  height: 100%;
  float: left;
  display: flex;
  background: #f5f5f5;
  
}
.menu_block_inner{
  width: 150rpx;
  display: flex;
  flex-direction: column;
  /*margin-top: 10rpx;*/
}
/*menuListF*/
.menuListF_items{
  width: 100%;
  height: 120rpx;
  display: flex;
  align-items: center;
  padding-left: 10rpx;
  font-size: 30rpx;
  border-bottom: 1px solid #eee;
}
.active{
  background: #fff;
  color: #9980DB;
}
/*ware_block*/
.ware_block{
  width:600rpx;
  /*height: auto;*/
  height: 100%;
  float: left;
  display: flex;
  flex-direction: column;
  /*align-items:flex-start;
  background: #fff;
  padding:10rpx 10rpx 10rpx 0;*/
}
.ware_block_inner{
  width:calc(600rpx - 20rpx);
  display: flex;
  flex-direction: column;
  align-items:flex-start;
  background: #fff;
  padding:0rpx 10rpx;
}
.column_tit{
  width: 580rpx;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.border{
  width: 8rpx;
  height: 50rpx;
  background: #9980DB;
  margin-right: 10rpx;
}
/*wareClip*/
.wareClip{
  width: 580rpx;
  height: auto;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.wareClip_item{
  width: 100%;
  /*width: 280rpx;*/
  height: 280rpx;
  display: flex;
  justify-content: space-between;
  margin-top: 30rpx;
}
